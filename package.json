{
  "name": "vue-picture-compressor",
  "version": "1.0.0",
  "description": "Vue 3 component for image compression with build-time optimization",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vite-plugin": {
      "import": "./src/vite-plugin.ts",
      "types": "./src/vite-plugin.ts"
    }
  },
  "files": [
    "dist",
    "src/vite-plugin.ts",
    "src/types",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vue",
    "vue3",
    "image",
    "compression",
    "picture",
    "optimization",
    "tiny",
    "tinypng"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pwplains9/vue-picture-compressor.git"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-webp": "^8.0.0",
    "imagemin-svgo": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0"
  }
}

